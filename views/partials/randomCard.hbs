<div class='container wrap'></div>
<div class='col-md-4'>
  <div class='card'>
      {{#each beer}}
    <img class='card-img-top' src={{this.image_url}} alt='{{name}}' />
    <div class='card-body'>
      <h5 class='card-title'>{{this.name}}</h5>
      <p class='card-text'>{{this.description}}</p>
    <p class='card-text'>{{this.tagline}}</p>
    </div>
  </div>
   {{/each}}
</div>
</div>